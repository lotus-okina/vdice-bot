    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@tsconfig/node16-strictest-esm": "^1.0.3",
    "@types/lodash": "^4.14.183",
    "@types/node": "^18.7.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "onchange": "^7.1.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "volta": {
    "node": "18.7.0"
  }
